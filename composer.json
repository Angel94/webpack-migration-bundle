{
    "name": "angel94/webpack-migration-bundle",
    "description": "Bundle to Help Migrating From Assetic to Webpack",
    "keywords": ["webpack", "assets", "assetic", "symfony", "symfony2", "amd", "commonjs"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marius BalÄytis",
            "email": "marius.balcytis@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:Angel94/twig-template-modification-bundle.git"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "maba/webpack-bundle": "*",
        "angel94/twig-template-modification-bundle": "*",
        "symfony/assetic-bundle": "^2.6",
        "symfony/framework-bundle": "^2.6||^3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Maba\\Bundle\\WebpackMigrationBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maba\\Tests\\": "test"
        }
    }
}
